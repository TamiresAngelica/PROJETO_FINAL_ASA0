;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ifrn.asa.br. root.asa.br. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2401200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN  MX 10 mail.ifrn.asa.br.
@	IN	NS	ns1.ifrn.asa.br.

ns1			A	192.168.0.6 
mail		A 	192.168.0.6 	
www			CNAME	ns1